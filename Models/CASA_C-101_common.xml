    <!-- Fuselaje -->

	<!-- Animacion de la turbina --> 
     <animation>
        <type>select</type>
        <object-name>Motor_01</object-name>
        <condition>
            <less-than>
                <property>engines/engine/thrust_lb</property>
                <value>1550</value>
            </less-than>
        </condition>
    </animation> 

    <animation>
        <type>spin</type>
        <object-name>Motor_01</object-name>
        <property>engines/engine/thrust_lb</property>
        <factor>1.0</factor>
        <center>
            <x-m> 2.65895 </x-m>
            <y-m> 0.0     </y-m>
            <z-m>-0.236833</z-m>
        </center>
        <axis>
            <x>-1.0</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

